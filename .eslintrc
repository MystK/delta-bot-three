{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "semi": [2, "never"],
    "no-console": [0],
    "no-case-declarations": [0],
    "no-fallthrough": [0],
    "linebreak-style": [0],
    "global-require": [0],
    "no-param-reassign": [2, {"props": false}],
    "import/prefer-default-export": [0]
  }
}
